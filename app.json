{
  "expo": {
    "name": "GR Packers Movers",
    "slug": "GRPackersMovers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/PackersMoversLogo.png",
    "scheme": "grpackersmovers",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to share live driver updates."
      }
    },
    
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/PackersMoversLogo.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.grpackersmovers"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/PackersMoversLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#050B18"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/PackersMoversLogo.png",
          "imageWidth": 180,
          "resizeMode": "contain",
          "backgroundColor": "#050B18",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "./plugins/with-text-recognition-sdk",
      "expo-secure-store",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
